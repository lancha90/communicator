<h1>Listing calls</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Opening</th>
    <th>Close</th>
    <th>Profile</th>
    <th>Work</th>
    <th>Hours week</th>
    <th>Hours total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calls.each do |call| %>
  <tr>
    <td><%= call.title %></td>
    <td><%= call.opening %></td>
    <td><%= call.close %></td>
    <td><%= call.profile %></td>
    <td><%= call.work %></td>
    <td><%= call.hours_week %></td>
    <td><%= call.hours_total %></td>
    <td><%= link_to 'Show', call %></td>
    <td><%= link_to 'Edit', edit_call_path(call) %></td>
    <td><%= link_to 'Destroy', call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Call', new_call_path %>
